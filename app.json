{
  "expo": {
    "name": "Experience",
    "slug": "Experience",
    "version": "0.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#202029",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#202029"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.example.Experience"
    },
    "androidNavigationBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#202029",
      "visible": "immersive"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "This app allows you to upload photos.",
          "cameraPermission": "This app allows you to take & upload photos."
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ]
  }
}